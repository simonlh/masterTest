@online{_nature_????,
	title = {The Nature of Design Practice and Implications for Interaction Design Research},
	url = {http://www.ijdesign.org/ojs/index.php/IJDesign/article/view/240},
	abstract = {International Journal of Design, {IJDesign}, {IJD}, {SCI}, {SSCI}, A\&{HCI}, The focus of this paper is{\textless}em{\textgreater} interaction design research aimed at supporting interaction design practice{\textless}/em{\textgreater}. The main argument is that this kind of interaction design research has not (always) been successful, and that the reason for this is that it has not been guided by a sufficient understanding of the {\textless}em{\textgreater}nature of design practice{\textless}/em{\textgreater}. Based on a comparison between the notion of {\textless}em{\textgreater}complexity{\textless}/em{\textgreater} in science and in design, it is argued that science is not the best place to look for approaches and methods on how to approach {\textless}em{\textgreater}design complexity{\textless}/em{\textgreater}. Instead, the case is made that any attempt by interaction design research to produce outcomes aimed at supporting design practice {\textless}em{\textgreater}must{\textless}/em{\textgreater} be grounded in a fundamental understanding of the nature of design practice. Such an understanding can be developed into a well-grounded and rich set of {\textless}em{\textgreater}rigorous{\textless}/em{\textgreater} and {\textless}em{\textgreater}disciplined {\textless}/em{\textgreater}design methods and techniques, appropriate to the needs and desires of practicing designers.},
	titleaddon = {International Journal of Dsign},
	urldate = {2014-11-29},
	file = {Snapshot:/Users/S-13/Library/Application Support/Zotero/Profiles/0o92n5lo.default/zotero/storage/MSQUUCXI/240.html:text/html}
