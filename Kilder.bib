@incollection{verbeek_morality_2008,
	title = {Morality in Design: Design Ethics and the Morality of Technological Artifacts},
	rights = {©2008 Springer Science+Business Media B.V.},
	isbn = {978-1-4020-6590-3, 978-1-4020-6591-0},
	url = {http://link.springer.com/chapter/10.1007/978-1-4020-6591-0_7},
	shorttitle = {Morality in Design},
	abstract = {A core issue in the philosophy of technology has been the non-neutrality of technology. Most scholars in the field agree that technologies actively help to shape culture and society, rather than being neutral means for realizing human ends. How to take seriously this non-neutrality of technology in ethics? Engineering ethics mainly focuses on the moral decisions and responsibilities of designers, and remains too external to the moral significance of technologies themselves. Yet, analyses of the non-neutrality of technology make it plausible to ascribe some morality to artifacts. First of all, technologies substantially contribute to the coming about of actions and of decisions about how to act. Second, their role cannot be entirely reduced to the intentions behind their design and use. This paper investigates what these observations imply for ethical theory, and for the ethics of design.},
	pages = {91--103},
	booktitle = {Philosophy and Design},
	publisher = {Springer Netherlands},
	author = {Verbeek, Peter-Paul},
	urldate = {2014-11-26},
	date = {2008-01-01},
	langid = {english},
	keywords = {Architecture, general, Engineering Design, ethics, Philosophy of Technology},
	file = {Full Text PDF:/Users/S-13/Library/Application Support/Zotero/Profiles/0o92n5lo.default/zotero/storage/87IGSW9U/Verbeek - 2008 - Morality in Design Design Ethics and the Morality.pdf:application/pdf;Snapshot:/Users/S-13/Library/Application Support/Zotero/Profiles/0o92n5lo.default/zotero/storage/UVN9HVST/10.html:text/html}
